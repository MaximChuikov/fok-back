{
  "compilerOptions": {
    "target": "es6",
    "module": "commonJS",
    "moduleResolution": "Node",
    "outDir": "compiled-js",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": true
  }
}